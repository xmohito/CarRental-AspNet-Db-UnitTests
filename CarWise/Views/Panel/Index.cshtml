@{
    ViewData["Title"] = "Panel";
}

@model IEnumerable<CarWise.Models.Customer>

    <div class="table-responsive">
        <table class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Surname</th>

                   

                </tr>
            </thead>
            <tbody>
                @foreach (var customer in Model)
                {
                    <tr>
                        <td>@customer.Name</td>
                        <td>@customer.Surname</td>

                    </tr>
                }
            </tbody>
        </table>x
    </div>
